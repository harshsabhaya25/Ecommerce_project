<select name="brandId" id="brandId">
    <option value="">--select Brand--</option>
    <% for(var b of brandData) {%>
        <option value="<%=b.id%>">
            <%=b.brand%>
        </option>
        <% }%>
</select>
<select name="typeId" id="typeId">
    <option value="">--select Type--</option>
    <% for(var t of typeData) {%>
        <option value="<%=t.id%>">
            <%=t.typeName%>
        </option>
        <% }%>
</select>