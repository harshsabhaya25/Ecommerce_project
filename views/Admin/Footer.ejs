<script>

 function getsubcategory()
 {
  var id = document.getElementById('categoryId').value;
  $.ajax({
    url:"/ExtraCategory/getsubcategory",
    method:"post",
    data:{
      'categoryId':id
    },
    success: function(res){
      // console.log(res);
      $('#subcategoryId').html(res);
    }
  })
 }

 function getExtraCategory(){
        var subId = document.getElementById('subcategoryId').value
        
        $.ajax({
          url : "/brand/getExtraCategory",
          method : "post",
          data : {  
            'subcategoryId' : subId
          },
          success : function(res){
            console.log(res);
            $('#Extracategory_name').html(res)
          }
        })
       }

  function getBrandType(){
        var catId = document.getElementById('categoryId').value
        var subId = document.getElementById('subcategoryId').value
        var extraId = document.getElementById('Extracategory_name').value

        $.ajax({
          url : "/Product/getBrandType",
          method : "post",
          data : {  
            'categoryId' : catId,
            'subCategoryId' : subId,
            'extraCategoryId' : extraId
          },
          success : function(res){
            $('#brandType').html(res)
          }
        })
  }      

</script>


